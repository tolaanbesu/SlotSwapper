{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/index.js" }
  ],
  "buildCommand": "cd server && npm install && npx prisma generate",
  "installCommand": "cd server && npm install"
}
